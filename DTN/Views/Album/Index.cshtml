@model IEnumerable<Album>

<h1>Albums</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
            <th>Release Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var album in Model)
        {
            <tr>
                <td>@album.Title</td>
                <td>@album.Artist</td>
                <td>@album.ReleaseDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
