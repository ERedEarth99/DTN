@model IEnumerable<DTN.Models.Song>

<h1>Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
            <th>Album</th>
            <th>Genre</th>
            <th>Duration</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var song in Model)
        {
            <tr>
                <td>@song.Title</td>
                <td>@song.Artist?.Name</td>
                <td>@song.Album?.Title</td>
                <td>@song.Genre?.Name</td>
                <td>@song.Duration</td>
            </tr>
        }
    </tbody>
</table>
