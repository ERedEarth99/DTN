@model DTN.Models.Album

<h1>Create Album</h1>

<form method="post">
    <label>Title:</label>
    <input type="text" asp-for="Title" /><br>
    <label>Artist:</label>
    <input type="text" asp-for="Artist" /><br>
    <label>Release Date:</label>
    <input type="date" asp-for="ReleaseDate" /><br>
    <button type="submit">Create</button>
</form>
